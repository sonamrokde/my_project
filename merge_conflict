public class TheMotivator {
	public void feedbackForTheScore(int score) {
		if (score == 100)
			System.out.println("You're genius. Good job!! I am proud of you”);
		else if (score > 90)
			System.out.println("That's great");
		else if (score > 60)
			System.out.println("That's good ");
		else
			System.out.println("Well, what can I say?");
	}
	public static void main(String[] args) {
		TheMotivator tm = new TheMotivator();
		tm.feedback(60);
	}
	public void upcomingEvents() {		System.out.println("Upcoming Events in the house”);		for (String event : events)
			System.out.println(event header);			System.out.println(event);	}
}

